{
    "name": "epics-db",
    "displayName": "epics-database",
    "description": "Colorize EPICS database and template files.",
    "version": "0.0.1",
    "publisher": "nsd",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "epics",
            "aliases": ["EPICS DB", "epics"],
            "extensions": [".db",".template"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "epics",
            "scopeName": "source.epics.db",
            "path": "./syntaxes/epics.tmLanguage.json"
        }]
    }
}