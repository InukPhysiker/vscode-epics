{
    "epicsEnvSet": {
        "prefix": "epics.epicsEnvSet",
        "body": "epicsEnvSet(\"${1:MACRO}\", \"${2:MACRO_VALUE}\") ${3}"
    },
    "require": {
        "prefix": "epics.require",
        "body": "require\t${1:module},${2:version} ${3}"
    },
    "dbLoadTemplate": {
        "prefix": "epics.dbLoadTemplate",
        "body": "dbLoadTemplate(${1:file}) ${3}"
    },
    "dbLoadRecords": {
        "prefix": "epics.dbLoadRecords",
        "body": "dbLoadRecords(${1:file}) ${3}"
    }
}